    "import sys, time\n",
    "from progressbar import *\n",
    " \n",
    "total = 1000\n",
    " \n",
    "def dosomework():\n",
    "    time.sleep(0.01)\n",
    "\n",
    "pbar = ProgressBar().start()\n",
    "for i in range(1000):\n",
    "    pbar.update(int((i / (total - 1)) * 100))\n",
    "    dosomework()\n",
    "pbar.finish()"
   ]
